# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'AdBlockVPNMacOS' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'KeychainAccess'
  pod 'Sparkle'
  pod 'SwiftyBeaver'
end

target 'AdBlockVPNMacOSProvider' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AdBlockVPNMacOSProvider
  pod 'OpenVPNAdapter', :git => 'https://github.com/betafish-inc/OpenVPNAdapterMirror', :branch => 'feature/use-queues-instead-of-runloop'
  pod 'Alamofire'
  pod 'KeychainAccess'
  pod 'SwiftyBeaver'
end
